    "version": "0.2.0",
    "configurations": [
        {
            "name": "debug-offline",
            "request": "launch",
            "type": "dart",
            "args": [
                "--dart-define=offline=true"
            ]
        },
        {
            "name": "profile-offline",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile",
            "args": [
                "--dart-define=offline=true"
            ]
        },
        {
            "name": "debug-online",
            "request": "launch",
            "type": "dart",
            "args": [
                "--dart-define=SECRET=secret"
            ]
        },
        {
            "name": "profile-online",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile",
            "args": [
                "--dart-define=SECRET=secret"
            ]
        }
    ]
}